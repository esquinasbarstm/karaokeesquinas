{
  "version": 2,
  "builds": [
    { "src": "api/buscar-video.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/buscar-video", "dest": "api/buscar-video.js" },
    { "src": "/index", "dest": "/index.html" },
    { "src": "/karaoke", "dest": "/karaoke.html" },
    { "src": "/console", "dest": "/console.html" },
    { "src": "/garcom", "dest": "/garcom.html" },
    { "src": "/karaoke-tv", "dest": "/karaoke-tv.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
